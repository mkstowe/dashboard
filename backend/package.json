{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "watch": "npx nodemon index.js",
    "pretty": "prettier --write \"./**/*.{js,json}\"",
    "db:migration": "knex migrate:make $migration --knexfile ./db/knexfile.js",
    "db:seed": "knex seed:make $seed --knexfile ./db/knexfile.js",
    "db:run": "cd db; knex migrate:down; knex migrate:up; knex seed:run"
  },
  "dependencies": {
    "@google-cloud/secret-manager": "^5.0.1",
    "@typescript-eslint/parser": "^5.0.1",
    "axios": "^1.5.0",
    "body-parser": "^1.20.2",
    "cockroachdb": "^1.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.7.0",
    "express": "^4.18.2",
    "express-oauth2-jwt-bearer": "^1.5.0",
    "knex": "^2.5.1",
    "nodemon": "^3.0.1",
    "pg": "^8.11.3",
    "prettier": "^2.5.1",
    "sqlite3": "^5.1.6",
    "typescript": "^4.4.4"
  }
}
